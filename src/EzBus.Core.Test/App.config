<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="destinations" type="EzBus.Core.Config.DestinationSection, EzBus.Core"/>
    <section name="subscriptions" type="EzBus.Core.Config.SubscriptionSection, EzBus.Core"/>
  </configSections>
  <destinations>
    <add assembly="Acme.Messages" endpoint="acme.input"/>
    <add assembly="Acme.Messages" message="Acme.Messages.DoAction" endpoint="acme.input.fastlane"/>
  </destinations>
  <subscriptions>
    <add endpoint="acme.sales"/>
    <add endpoint="acme.production"/>
  </subscriptions>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
